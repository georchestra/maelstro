{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "vue-i18n-extract": "vue-i18n-extract report --add --remove --vueFiles './src/**/*.?(ts|vue)' --languageFiles './src/locales/*.?(json|yml|yaml|js)'"
  },
  "dependencies": {
    "@primevue/forms": "^4.2.5",
    "@primevue/themes": "^4.2.5",
    "pinia": "^2.3.1",
    "primeicons": "^7.0.0",
    "primelocale": "^1.6.0",
    "primevue": "^4.2.5",
    "vue": "^3.5.13",
    "vue-i18n": "^10.0.5",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.7",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/eslint-plugin": "1.1.20",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.20.1",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.2",
    "prettier": "^3.5.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-primeui": "^0.4.0",
    "typescript": "~5.6.3",
    "vite": "^6.1.0",
    "vite-plugin-vue-devtools": "^7.7.1",
    "vitest": "^2.1.9",
    "vue-i18n-extract": "^2.0.7",
    "vue-tsc": "^2.2.0"
  }
}
